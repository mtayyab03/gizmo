    "android": {
      "package": "com.tayyab_gondal.gizmo",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7d9767af-9fde-4690-882d-dca0f3f3e19e"
      }
    }
  }
}
